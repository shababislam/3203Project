import java.awt.*;

import javax.swing.JPanel;

public class Graph extends JPanel {
	
	int x = 1;
	//int y = 1;
	private Main3203 main;
	
	public Graph(Main3203 main){
		this.main = main;
	}
	
	
	public void paint(Graphics g){
		g.drawLine(0, 275, 800, 275);
		g.drawRect(x, 250, 50, 50);
		
	}
	
	void Update(){
		System.out.println(x);
		if(x>=700)
			x = 100;
		x++;
		}
	}
	
}
