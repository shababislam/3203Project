import javax.swing.*;
import java.awt.*;

public class Main3203 extends JFrame{

	Graph g = new Graph(this);
	
	public static void main(String[] args) {
		new Main3203();
	}
	
	private void Update(){
		g.Update();
	}
	
	public Main3203(){
		setSize(800,600);
		setVisible(true);
		initialize();
	}
	
	private void initialize(){
		setLayout(new GridLayout(1,1,0,0));
		g = new Graph(this);
		add(g);
		setVisible(true);
		
	}

}
